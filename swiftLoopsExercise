for i in 1...7 {
    print(i)
}

let greetings = [
"Hello",
"Hi",
"What's up"
]

for i in 0..<greetings.count {
    print(greetings[i])
}

var num1 = 5

while(num1 <= 25) {
    print(num1)
    num1 += 1
}

var num2 = 2
repeat {
    print("Hello World")
    num2 = num2 + 1
} while (num2 < 2)
// Function will print "Hello World" once, bool condition is false after first loop

